BigEndian();

typedef struct {
    char name[4];
    unsigned int size;
    unsigned int unknown00;
    unsigned int unknown01;
} header;

header CHM_header;

local int data_start;
local int i;
while((FTell() + sizeof(header)) < FileSize()){
    header new_header;
    data_start = FTell();
    
    for(i = 0; i < new_header.unknown00 - 1; i++){
        header sub_header;
        FSkip(sub_header.size);
    }
    FSeek(data_start + new_header.size);
}

